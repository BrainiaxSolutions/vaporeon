{
    "name": "vaporeon",
    "version": "1.0.0",
    "description": "Lambda responsável pelo gerenciamento de alertas do sistema Pluvial.",
    "author": "Beatriz Assunção, Luiz Ricardo Santos, Ian Soares e Yumi Tokuo",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
      "build": "nest build",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "lambda:start:deploy": "serverless deploy",
      "lambda:start:dev": "npm run build && npx serverless offline start --reloadHandler",
      "start": "nest start",
      "start:dev": "nest start --watch",
      "start:debug": "nest start --debug --watch",
      "start:prod": "node dist/main",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
      "@fastify/static": "^6.10.1",
      "aws-lambda": "^1.0.7",
      "aws-lambda-fastify": "^2.2.0",
      "mongoose": "^7.2.2",
      "node-geocoder": "^4.2.0",
      "serverless": "^3.34.0",
      "axios": "^1.5.0"
    },
    "jest": {
      "moduleFileExtensions": [
        "js",
        "json",
        "ts"
      ],
      "rootDir": "src",
      "testRegex": ".*\\.spec\\.ts$",
      "transform": {
        "^.+\\.(t|j)s$": "ts-jest"
      },
      "collectCoverageFrom": [
        "**/*.(t|j)s"
      ],
      "coverageDirectory": "../coverage",
      "testEnvironment": "node"
    }
  }
  